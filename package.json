{
  "name": "bad-code-blaster",
  "version": "1.0.0",
  "description": "Client and server packages for bad-code-blaster.",
  "main": "server/server.js",
  "scripts": {
    "develop": "concurrently \"cd server && npm run start\" \"cd client && npm start\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "install-client": "cd client && npm ci",
    "install-server": "cd server && npm ci",
    "build": "npm run install-client && npm run install-server && cd client && npm run build",
    "start": "npm run develop"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lanebpemberton/bad-code-blaster.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lanebpemberton/bad-code-blaster/issues"
  },
  "homepage": "https://github.com/lanebpemberton/bad-code-blaster#readme",
  "devDependencies": {
    "concurrently": "^6.2.0"
  }
}
